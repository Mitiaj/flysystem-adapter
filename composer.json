{
  "name": "betarpininku/flysystem-adapter",
  "description": "Filesystem adapter through betarpininku api",
  "keywords": ["repository", "laravel"],
  "type": "project",

  "autoload": {
    "psr-4": {
      "BT\\FlysystemAdapter\\" : "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },

  "require-dev": {
    "php": ">=7.1.0",
    "league/flysystem": "^1.0",
    "guzzlehttp/guzzle": "^6.2"
    "phpunit/phpunit": "~5.0"
  },
  "suggest": {
    "league/flysystem": "^1.0",
    "guzzlehttp/guzzle": "^6.2"
  }
}
